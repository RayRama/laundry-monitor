{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "login/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "monitor/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "leaderboard/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/**",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server.js"
    },
    {
      "src": "/dashboard",
      "dest": "dist/server.js"
    },
    {
      "src": "/leaderboard",
      "dest": "dist/server.js"
    },
    {
      "src": "/monitor",
      "dest": "dist/server.js"
    },
    {
      "src": "/login",
      "dest": "login/index.html"
    },
    {
      "src": "/styles/(.*)",
      "dest": "styles/$1"
    },
    {
      "src": "/scripts/(.*)",
      "dest": "scripts/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "assets/$1"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
